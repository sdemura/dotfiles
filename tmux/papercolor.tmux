# This tmux statusbar config was created by tmuxline.vim
# on Mon, 04 Mar 2019

set -g status-justify "left"
set -g status "on"
set -g status-attr "none"
set -g message-command-bg "#d0d0d0"
set -g status-left-length "100"
set -g pane-active-border-fg "#005f87"
set -g status-bg "#005f87"
set -g message-command-fg "#444444"
set -g pane-border-fg "#d0d0d0"
set -g message-bg "#d0d0d0"
set -g status-left-attr "none"
set -g status-right-attr "none"
set -g status-right-length "100"
set -g message-fg "#444444"
setw -g window-status-fg "#e4e4e4"
setw -g window-status-attr "bold"
setw -g window-status-activity-bg "#005f87"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "#e4e4e4"
setw -g window-status-separator ""
setw -g window-status-bg "#005f87"
set -g status-left "#[fg=#e4e4e4,bg=#005f87,bold] #S #[fg=#005f87,bg=#005f87,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#005f87,bg=#005f87,nobold,nounderscore,noitalics]#[fg=#e4e4e4,bg=#005f87,bold] %Y-%m-%d | %H:%M #[fg=#005f87,bg=#005f87,nobold,nounderscore,noitalics]#[fg=#e4e4e4,bg=#005f87,bold] #h "
setw -g window-status-format "#[fg=#e4e4e4,bg=#005f87,bold] #I |#[fg=#e4e4e4,bg=#005f87,bold] #W "
setw -g window-status-current-format "#[fg=#005f87,bg=#d0d0d0,nobold,nounderscore,noitalics]#[fg=#444444,bg=#d0d0d0] #I |#[fg=#444444,bg=#d0d0d0] #W #[fg=#d0d0d0,bg=#005f87,nobold,nounderscore,noitalics]"
